function prime(){
  flag = true;
  if(num < 2)
    flag = false;
    for(i = 2; i <= Math.sqrt(num); i++){
      if(num % i == 0){
        flag = false;
        break;
      }
    }
  if(flag){
    console.log("Number "+ num + " is prime")
  }
  else{
    console.log("Number "+ num + " is not a prime")
  }
}
