function isprime(num){
  flag = true;
  if(num < 2)
    flag = false;
    for(i = 2; i <= Math.sqrt(num); i++){
      if(num % i == 0){
        flag = false;
        break;
      }
    }
  return flag;
  }
function prime(lb, ub){
  console.log("Prime numbers between " +lb + " & " + ub + " is: ")
  for(var i = lb; i<= ub; i++){
    if(isprime(i)){
      console.log(  i)
    }
  }
}
prime(1, 25)
